FORM DB_UPD_YSD_V003_DESCUEN .
*.process data base updates/inserts/deletes.........................*
LOOP AT TOTAL.
  CHECK <ACTION> NE ORIGINAL.
MOVE <VIM_TOTAL_STRUC> TO YSD_V003_DESCUEN.
  IF <ACTION> = UPDATE_GELOESCHT.
    <ACTION> = GELOESCHT.
  ENDIF.
  CASE <ACTION>.
   WHEN NEUER_GELOESCHT.
IF STATUS_YSD_V003_DESCUEN-ST_DELETE EQ GELOESCHT.
     READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
     IF SY-SUBRC EQ 0.
       DELETE EXTRACT INDEX SY-TABIX.
     ENDIF.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN GELOESCHT.
  SELECT SINGLE FOR UPDATE * FROM YSD_T003_DESCUEN WHERE
  VKORG = YSD_V003_DESCUEN-VKORG AND
  TIPODOC = YSD_V003_DESCUEN-TIPODOC AND
  SECUENCIA = YSD_V003_DESCUEN-SECUENCIA AND
  KSCHL = YSD_V003_DESCUEN-KSCHL .
    IF SY-SUBRC = 0.
    DELETE YSD_T003_DESCUEN .
    ENDIF.
    IF STATUS-DELETE EQ GELOESCHT.
      READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY> BINARY SEARCH.
      DELETE EXTRACT INDEX SY-TABIX.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN OTHERS.
  SELECT SINGLE FOR UPDATE * FROM YSD_T003_DESCUEN WHERE
  VKORG = YSD_V003_DESCUEN-VKORG AND
  TIPODOC = YSD_V003_DESCUEN-TIPODOC AND
  SECUENCIA = YSD_V003_DESCUEN-SECUENCIA AND
  KSCHL = YSD_V003_DESCUEN-KSCHL .
    IF SY-SUBRC <> 0.   "insert preprocessing: init WA
      CLEAR YSD_T003_DESCUEN.
    ENDIF.
YSD_T003_DESCUEN-MANDT =
YSD_V003_DESCUEN-MANDT .
