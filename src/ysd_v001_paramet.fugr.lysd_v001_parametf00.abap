  ENDIF.
  REFRESH EXTRACT.
ENDFORM.
*---------------------------------------------------------------------*
FORM DB_UPD_YSD_V001_PARAMET .
*.process data base updates/inserts/deletes.........................*
LOOP AT TOTAL.
  CHECK <ACTION> NE ORIGINAL.
MOVE <VIM_TOTAL_STRUC> TO YSD_V001_PARAMET.
  IF <ACTION> = UPDATE_GELOESCHT.
    <ACTION> = GELOESCHT.
  ENDIF.
  CASE <ACTION>.
   WHEN NEUER_GELOESCHT.
IF STATUS_YSD_V001_PARAMET-ST_DELETE EQ GELOESCHT.
     READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
     IF SY-SUBRC EQ 0.
       DELETE EXTRACT INDEX SY-TABIX.
     ENDIF.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN GELOESCHT.
  SELECT SINGLE FOR UPDATE * FROM YSD_T001_PARAMET WHERE
  DOC_TYPE_C = YSD_V001_PARAMET-DOC_TYPE_C AND
  DOC_TYPE_P = YSD_V001_PARAMET-DOC_TYPE_P AND
  DOC_TYPE_F = YSD_V001_PARAMET-DOC_TYPE_F AND
  INDI_EXPEDI = YSD_V001_PARAMET-INDI_EXPEDI AND
  FACTURACION = YSD_V001_PARAMET-FACTURACION AND
  VKORG = YSD_V001_PARAMET-VKORG .
    IF SY-SUBRC = 0.
    DELETE YSD_T001_PARAMET .
    ENDIF.
    IF STATUS-DELETE EQ GELOESCHT.
      READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY> BINARY SEARCH.
      DELETE EXTRACT INDEX SY-TABIX.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN OTHERS.
  SELECT SINGLE FOR UPDATE * FROM YSD_T001_PARAMET WHERE
  DOC_TYPE_C = YSD_V001_PARAMET-DOC_TYPE_C AND
  DOC_TYPE_P = YSD_V001_PARAMET-DOC_TYPE_P AND
  DOC_TYPE_F = YSD_V001_PARAMET-DOC_TYPE_F AND
  INDI_EXPEDI = YSD_V001_PARAMET-INDI_EXPEDI AND
  FACTURACION = YSD_V001_PARAMET-FACTURACION AND
  VKORG = YSD_V001_PARAMET-VKORG .
    IF SY-SUBRC <> 0.   "insert preprocessing: init WA
      CLEAR YSD_T001_PARAMET.
    ENDIF.
YSD_T001_PARAMET-MANDT =
YSD_V001_PARAMET-MANDT .
YSD_T001_PARAMET-DOC_TYPE_C =
YSD_V001_PARAMET-DOC_TYPE_C .
YSD_T001_PARAMET-DOC_TYPE_P =
YSD_V001_PARAMET-DOC_TYPE_P .
YSD_T001_PARAMET-DOC_TYPE_F =
YSD_V001_PARAMET-DOC_TYPE_F .
YSD_T001_PARAMET-INDI_EXPEDI =
YSD_V001_PARAMET-INDI_EXPEDI .
YSD_T001_PARAMET-FACTURACION =
YSD_V001_PARAMET-FACTURACION .
YSD_T001_PARAMET-VKORG =
YSD_V001_PARAMET-VKORG .
YSD_T001_PARAMET-UNAME =
YSD_V001_PARAMET-UNAME .
YSD_T001_PARAMET-AEDTM =
YSD_V001_PARAMET-AEDTM .
YSD_T001_PARAMET-UZEIT =
YSD_V001_PARAMET-UZEIT .
    IF SY-SUBRC = 0.
    UPDATE YSD_T001_PARAMET ##WARN_OK.
    ELSE.
    INSERT YSD_T001_PARAMET .
    ENDIF.
    READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
    IF SY-SUBRC EQ 0.
      <XACT> = ORIGINAL.
      MODIFY EXTRACT INDEX SY-TABIX.
    ENDIF.
    <ACTION> = ORIGINAL.
    MODIFY TOTAL.
  ENDCASE.
ENDLOOP.
CLEAR: STATUS_YSD_V001_PARAMET-UPD_FLAG,
STATUS_YSD_V001_PARAMET-UPD_CHECKD.
MESSAGE S018(SV).
ENDFORM.
